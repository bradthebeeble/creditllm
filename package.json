{
  "name": "creditllm-slack-bot",
  "version": "1.0.0",
  "description": "Slack bot with Playwright automation for financial account management",
  "main": "dist/src/bot.js",
  "scripts": {
    "start": "node dist/src/bot.js",
    "dev": "npm run build && npm run start",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "playwright:install": "npx playwright install",
    "setup": "npm install && npm run playwright:install"
  },
  "keywords": [
    "slack",
    "bot",
    "playwright",
    "automation",
    "banking",
    "financial",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^3.14.0",
    "cron": "^2.4.4",
    "dotenv": "^16.3.1",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/cron": "^2.4.0",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bradthebeeble/creditllm.git"
  },
  "bugs": {
    "url": "https://github.com/bradthebeeble/creditllm/issues"
  },
  "homepage": "https://github.com/bradthebeeble/creditllm#readme"
}
